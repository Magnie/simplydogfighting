<DogfightGame>:
    ProgressBar:
        id: health
        max: 100
        value: 75
    FloatLayout:
        id: canvas
        #GenericObject

<GenericObject>:
    source: root.source
    canvas.before:
        PushMatrix
        Rotate:
            angle: root.angle
            axis: 0, 0, 1
            origin: root.center
    canvas.after:
        PopMatrix
